# Generic aliases
alias reload!='source ~/.zshrc'
alias ls='ls --color=auto -l'
alias df='df -h' # disk free
alias du='du -h -c' # space usage for folder

# Tmux aliases
alias ta='tmux attach'
alias tat='tmux attach -t'
alias tls='tmux ls'
alias tn='tmux new-session -s'

# Livestreamer aliases
alias sevadus='livestreamer twitch.tv/sevadus'
alias dansgaming='livestreamer twitch.tv/dansgaming'
alias towelliee='livestreamer twitch.tv/towelliee'
